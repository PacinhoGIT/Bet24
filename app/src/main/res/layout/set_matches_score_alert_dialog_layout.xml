<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:text="@string/match"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:id="@+id/textView"
        android:textSize="22dp"
        android:layout_alignParentStart="true"
        android:textColor="@color/colorPrimaryDark"
        android:textAlignment="center"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="29dp"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/matchDateAlertDialog"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="@string/score"
        android:textAlignment="center"
        android:textSize="22dp"
        android:layout_width="wrap_content"
        android:textColor="@color/colorPrimaryDark"
        android:layout_height="wrap_content"
        android:layout_below="@+id/matchDateAlertDialog"
        android:layout_alignParentStart="true"
        android:layout_marginTop="36dp"
        android:layout_alignParentEnd="true"
        android:id="@+id/scoreTV" />

    <TextView
        android:text="TextView"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:singleLine="false"
        android:lines="2"
        android:maxLines="2"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignTop="@+id/teamAAlertDialog"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="37dp"
        android:id="@+id/teamBAlertDialog" />

    <TextView
        android:text="TextView"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:singleLine="false"
        android:lines="2"
        android:maxLines="2"
        android:layout_marginTop="36dp"
        android:id="@+id/teamAAlertDialog"
        android:textColor="@color/colorPrimaryDark"
        android:layout_below="@+id/scoreTV"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:textAlignment="center"
        android:layout_marginLeft="200dp"
        android:id="@+id/resultBAlertDialogET"
        android:layout_alignBaseline="@+id/resultAAlertDialogET"
        android:layout_alignBottom="@+id/resultAAlertDialogET"
        android:layout_alignEnd="@+id/teamBAlertDialog" />

    <TextView
        android:text=":"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_alignBaseline="@+id/resultAAlertDialogET"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignBottom="@+id/resultAAlertDialogET"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:id="@+id/matchNameAlertDialog"
        android:textColor="@color/colorPrimaryDark"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:srcCompat="@drawable/team"
        android:id="@+id/teamALogosetScoreIV"
        android:layout_marginStart="12dp"
        android:layout_marginTop="9dp"
        android:layout_below="@+id/matchNameAlertDialog"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:srcCompat="@drawable/team"
        android:id="@+id/teamBLogoSetScoreIV"
        android:layout_alignTop="@+id/teamALogosetScoreIV"
        android:layout_toEndOf="@+id/resultBAlertDialogET" />

    <TextView
        android:text="@string/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="22dp"
        android:id="@+id/textView4"
        android:layout_below="@+id/teamALogosetScoreIV"
        android:layout_alignEnd="@+id/teamBLogoSetScoreIV"
        android:layout_marginTop="20dp"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:layout_marginTop="17dp"
        android:layout_marginRight="200dp"
        android:textAlignment="center"
        android:id="@+id/resultAAlertDialogET"
        android:layout_below="@+id/teamAAlertDialog"
        android:layout_alignStart="@+id/teamAAlertDialog" />

</RelativeLayout>